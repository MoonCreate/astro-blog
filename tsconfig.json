{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "#root/*": ["src/*"],
      "#components/*": ["src/components/*"],
      "#layouts/*": ["src/layouts/*"],
      "#content/*": ["src/content/*"],
      "#styles/*": ["src/styles/*"],
    }
  }
}
